<form action="/login" method="POST">
  <input type="hidden" name="_method" value="PUT">
  <input type="submit" value="Logout">
</form>
<h1>Deck</h1>
  <h3> <%=@deck.name %></h3>
  <h5> <%=@deck.description %></h5>
  <% @cards.each do |quest| %>
    <ul class="list">
      <li>Question: <%=quest.question%></li>
    </ul>
    <hr>
  <% end %>
  <a href="/decks/<%= @deck.id%>/cards/0">Let's fuckin play! </a>

